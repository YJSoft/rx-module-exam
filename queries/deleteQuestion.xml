<query id="deleteQuestion" action="delete">
    <tables>
        <table name="exam_question" />
    </tables>
    <conditions>
        <condition operation="equal" column="question_srl" var="question_srl" filter="number" notnull="notnull" />
        <condition operation="equal" column="document_srl" var="document_srl" filter="number" pipe="and" />
    </conditions>
</query>
