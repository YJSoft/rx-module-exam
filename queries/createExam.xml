<query id="createExam" action="insert">
    <tables>
        <table name="exam" />
    </tables>
    <columns>
        <column name="document_srl" var="document_srl" filter="number" notnull="notnull" />
        <column name="module_srl" var="module_srl" filter="number" notnull="notnull" />
        <column name="category_srl" var="category_srl" default="0" filter="number" />
        <column name="member_srl" var="member_srl" default="0" filter="number" />
        <column name="user_name" var="user_name" default="" />
        <column name="nick_name" var="nick_name" default="" />
        <column name="title" var="title" notnull="notnull" />
        <column name="title_bold" var="title_bold" default="" />
        <column name="title_color" var="title_color" default="" />
        <column name="content" var="content" default="" />
        <column name="page_type" var="page_type" default="0" filter="number" />
        <column name="result_type" var="result_type" default="0" filter="number" />
        <column name="cutline" var="cutline" default="0" filter="number" />
        <column name="is_time" var="is_time" default="N" />
        <column name="is_date" var="is_date" default="N" />
        <column name="join_point" var="join_point" default="0" filter="number" />
        <column name="exam_time" var="exam_time" default="0" filter="number" />
        <column name="question_count" default="0" filter="number" />
        <column name="join_count" default="0" filter="number" />
        <column name="start_date" var="start_date" default="" />
        <column name="end_date" var="end_date" default="" />
        <column name="regdate" var="regdate" default="curdate()" />
        <column name="ipaddress" var="ipaddress" default="ipaddress()" />
        <column name="list_order" var="list_order" default="0" />
        <column name="status" var="status" default="Y" />
        <column name="pass_point" var="pass_point" default="0" />
        <column name="pass_group_list" var="pass_group_list" default="" />
    </columns>
</query>
