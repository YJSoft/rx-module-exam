    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="log_srl" var="log_srl" filter="number" />
        <condition operation="equal" column="document_srl" var="document_srl" filter="number" pipe="and" />
    </conditions>
</query>
